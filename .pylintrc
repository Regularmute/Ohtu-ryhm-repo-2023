[MASTER]

ignore=tests,
        AppLibrary.py

[MESSAGE CONTROL]

disable=raw-checker-failed,
        bad-inline-option,
        locally-disabled,
        file-ignored,
        suppressed-message,
        useless-suppression,
        deprecated-pragma,
        use-symbolic-message-instead,
        missing-module-docstring,
        missing-function-docstring,
        missing-class-docstring

[IMPORTS]

disable=unused-import, wrong-import-position, wrong-import-order, import-error